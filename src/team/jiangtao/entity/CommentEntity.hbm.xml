<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.jiangtao.entity.CommentEntity" table="comment" schema="ssmis">
        <composite-id mapped="true" class="team.jiangtao.entity.CommentEntityPK">
            <key-property name="dpm">
                <column name="dpm" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="crs">
                <column name="crs" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="tch">
                <column name="tch" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="date">
                <column name="date" sql-type="date"/>
            </key-property>
        </composite-id>
        <property name="content">
            <column name="content" sql-type="varchar(128)" length="128"/>
        </property>
    </class>
</hibernate-mapping>