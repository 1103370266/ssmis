<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.jiangtao.entity.DepartmentEntity" table="department" schema="ssmis">
        <id name="dpmId">
            <column name="dpm_id" sql-type="char(8)" length="8"/>
        </id>
        <property name="dpmName">
            <column name="dpm_name" sql-type="varchar(32)" length="32"/>
        </property>
    </class>
</hibernate-mapping>