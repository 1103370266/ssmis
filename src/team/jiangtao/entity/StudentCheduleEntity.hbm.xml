<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.jiangtao.entity.StudentCheduleEntity" table="student_chedule" schema="ssmis">
        <composite-id mapped="true" class="team.jiangtao.entity.StudentCheduleEntityPK">
            <key-property name="dpm">
                <column name="dpm" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="crs">
                <column name="crs" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="tch">
                <column name="tch" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="stu">
                <column name="stu" sql-type="char(8)" length="8"/>
            </key-property>
        </composite-id>
        <property name="term">
            <column name="term" sql-type="tinyint(4)"/>
        </property>
        <property name="score">
            <column name="score" sql-type="float" precision="-1"/>
        </property>
        <many-to-one name="departmentByDpm" class="team.jiangtao.entity.DepartmentEntity">
            <column name="dpm"/>
        </many-to-one>
        <many-to-one name="courseByCrs" class="team.jiangtao.entity.CourseEntity">
            <column name="crs"/>
        </many-to-one>
        <many-to-one name="studentByStu" class="team.jiangtao.entity.StudentEntity">
            <column name="stu"/>
        </many-to-one>
    </class>
</hibernate-mapping>