<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.jiangtao.entity.CoursesTableEntity" table="courses_table" schema="ssmis">
        <composite-id mapped="true" class="team.jiangtao.entity.CoursesTableEntityPK">
            <key-property name="dpmId">
                <column name="dpm_id" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="crsId">
                <column name="crs_id" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="tchId">
                <column name="tch_id" sql-type="char(8)" length="8"/>
            </key-property>
            <key-property name="weeks">
                <column name="weeks" sql-type="tinyint(4)"/>
            </key-property>
            <key-property name="off">
                <column name="off" sql-type="tinyint(4)"/>
            </key-property>
        </composite-id>
        <many-to-one name="courseByCrsId" class="team.jiangtao.entity.CourseEntity">
            <column name="crs_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>