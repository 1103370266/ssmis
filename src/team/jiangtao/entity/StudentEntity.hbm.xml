<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.jiangtao.entity.StudentEntity" table="student" schema="ssmis">
        <id name="stuId">
            <column name="stu_id" sql-type="char(8)" length="8"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(13)" length="13"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="date"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="tinyint(1)"/>
        </property>
        <property name="grade">
            <column name="grade" sql-type="tinyint(4)"/>
        </property>
        <property name="classNo">
            <column name="class_no" sql-type="tinyint(4)"/>
        </property>
        <property name="stuStatus">
            <column name="stu_status" sql-type="tinyint(4)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>