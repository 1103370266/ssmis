<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.jiangtao.entity.CourseEntity" table="course" schema="ssmis">
        <id name="crsId">
            <column name="crs_id" sql-type="char(8)" length="8"/>
        </id>
        <property name="crsName">
            <column name="crs_name" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="summarization">
            <column name="summarization" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>